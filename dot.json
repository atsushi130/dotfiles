[
  {
    "name": "vimrc",
    "type": "file",
    "input": "/vim/.vimrc",
    "output": "~/.vimrc",
    "chain": [
      "ideavimrc", "xvimrc", "vim.d", "dein"
    ]
  },
  {
    "name": "vim.d",
    "type": "dir",
    "input": "/vim/.vim.d",
    "output": "~/.vim.d"
  },
  {
    "name": "dein",
    "type": "dir",
    "input": "/vim/dein",
    "output": "~/.vim/dein"
  },
  {
    "name": "deinrc",
    "type": "file",
    "input": "/vim/dein/.deinrc",
    "output": "~/.vim/dein/.deinrc"
  },
  {
    "name": "dein.toml",
    "type": "file",
    "input": "/vim/dein/dein.toml",
    "output": "~/.vim/dein/dein.toml",
    "chain": [
        "plugins"
    ]
  },
  {
    "name": "dein_lazy.toml",
    "type": "file",
    "input": "/vim/dein/dein_lazy.toml",
    "output": "~/.vim/dein/dein_lazy.toml",
