  },
  {
    "name": "setting.vim",
    "type": "file",
    "input": "/vim/.vim.d/setting.vim",
    "output": "~/.vim.d/setting.vim"
  },
  {
    "name": "zshrc",
    "type": "file",
    "input": "/zsh/.zshrc",
    "output": "~/.zshrc",
    "chain": [
      "zsh.d"
    ]
  },
  {
    "name": "zsh.d",
